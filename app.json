{
  "expo": {
    "name": "react-samu",
    "slug": "react-samu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/samuIcon.png",
    "scheme": "myapp2",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/samuSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/samuIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ericksousa312.reactsamu"
    },
    "web": {
      "favicon": "./assets/samuIcon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f0deb4f0-481c-4ea8-b546-52383520d974"
      }
    }
  }
}
